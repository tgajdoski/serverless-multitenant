{
    "env": {
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "warn",
            2
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-console": [
            0
        ],
        "no-unused-vars": [
            1
        ],
        "no-unexpected-multiline": [
            1
        ]
    }
}